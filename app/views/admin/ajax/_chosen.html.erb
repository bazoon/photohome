<div id="selected">
<% if @photos %>
<% @photos.each do |photo| %>
<%= link_to(image_tag(photo.image_url(:thumb), style: "float: left; height: 50px;", class: "img-responsive"), admin_remove_from_chosen_path(photo.id),remote: true, method: :put) %>
<% end %>
<% end %>
<div class="clearfix"></div>
</div> 

 