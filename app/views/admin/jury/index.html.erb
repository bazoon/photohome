<h1>Жюри</h1>
  <%= simple_form_for [@competition,@jury], :url => admin_update_jury_path(@competition), :html => {:method => :put, :class => 'form-horizontal' } do |f| %>

      <%= f.input :user_tokens,label: "Новый член жюри", :input_html => {data: {load: ""   }} %>

      <%= f.button :submit,class: "btn btn-default"  ,style: "visibility: hidden" %>
   <% end %>


<%= link_to 'Добавить всех участников', move_all_admin_competition_jury_index_path(@competition), method: :post, remote: true, class: 'btn btn-warning' %>



<%= link_to 'Удалить всех',delete_all_admin_competition_jury_index_path(@competition), method: :delete, remote: true, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %>


<%= render partial: 'admin/jury/jury_table', locals: { jurys: @jurys, competition: @competition }%>